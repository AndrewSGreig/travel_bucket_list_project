<h1>Countries</h1>


<table>
  <tr>
    <th>Name</th>
    <th>|</th>
    <th>Continent</th>
    <th>|</th>
    <th>Visited?</th>
    <th>|</th>
    <th>Delete</th>
  </tr>
  <% for country in @countries %>
  <tr>
    <td><a href="/country/<%= country.id %>/edit"><%= country.name %></a></td>
    <td>|</td>
    <td><%= country.continent %></td>
    <td>|</td>
    <td><%= country.visited %></td>
    <td>|</td>
    <td><a href="/country/<%= country.id %>/delete">Delete <%= country.name %></a></td>
  </tr>
  <% end %>
</table>
