<h1>Find Destination By Name</h1>
<form method="POST" action="/country/destination">
  <label for="name"> Destination Name: </label>
  <input type="text" name="name" id="name" disabled=true value="<%= @dest_find_results[0].name %>"/>

  <label for="type"> Destination Type: </label>
  <input type="text" name="type" id="type" disabled=true value="<%= @dest_find_results[0].type %>"/>

  <label for="country"> Destination Country: </label>
  <input type="text" name="country" id="country" disabled=true value="<%= @dest_find_results[0].country.name %>">


</form>
